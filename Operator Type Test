void main() {
  var AndilName = "Andil"; // Tipe data String
  var AndilAge = 21;       // Tipe data int
  var AndilHeight = 175.5; // Tipe data double

  // Menggunakan operator type test
  print('Apakah AndilName adalah String? ${AndilName is String}'); // true
  print('Apakah AndilAge adalah int? ${AndilAge is int}');         // true
  print('Apakah AndilHeight adalah double? ${AndilHeight is double}'); // true
  print('Apakah AndilHeight adalah int? ${AndilHeight is int}'); // false

  // Menggunakan operator type test untuk memeriksa tipe dan cast
  if (AndilName is String) {
    print('Nama saya adalah ' + AndilName + ' dengan panjang ' + AndilName.length.toString() + ' karakter.');
  }
}
