void main() {
  // Membuat set dengan nama "Andil" untuk menyimpan nama-nama
  Set<String> AndilNames = {'Aji', 'Budi', 'Cici', 'Aji'}; // 'Aji' akan otomatis dihapus karena set tidak mengizinkan duplikasi

  // Menampilkan isi set
  print('Nama-nama dalam set Andil: ' + AndilNames.toString());

  // Menambahkan nama baru
  AndilNames.add('Doni');
  print('Setelah menambahkan Doni: ' + AndilNames.toString());

  // Menghapus nama
  AndilNames.remove('Budi');
  print('Setelah menghapus Budi: ' + AndilNames.toString());

  // Mengecek apakah nama ada dalam set
  print('Apakah Cici ada dalam set? ' + (AndilNames.contains('Cici') ? 'Ya' : 'Tidak'));
  print('Apakah Aji ada dalam set? ' + (AndilNames.contains('Aji') ? 'Ya' : 'Tidak'));

  // Menampilkan jumlah nama dalam set
  print('Jumlah nama dalam set Andil: ' + AndilNames.length.toString());
}
