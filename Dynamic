void main() {
  dynamic AndilData = 30;  // Memulai dengan nilai integer
  print('Nilai awal Andil: $AndilData, tipe: ${AndilData.runtimeType}');

  AndilData = "Halo, saya Andil!";  // Mengubah menjadi string
  print('Pesan baru Andil: $AndilData, tipe: ${AndilData.runtimeType}');

  AndilData = [1, 2, 3, "empat"];  // Mengubah menjadi list
  print('List baru Ferdi: $AndilData, tipe: ${AndilData.runtimeType}');
}
