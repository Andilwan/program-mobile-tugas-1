void main() {
  // Daftar nama yang hanya berisi nama ANDILWAN
  List<String> names = ['ANDILWAN'];

  // Menggunakan fungsi anonim untuk memfilter nama yang mengandung "ANDIL"
  var filteredNames = names.where((name) => name.contains('ANDIL')).toList();

  print('Nama yang mengandung "ANDIL":');
  filteredNames.forEach((name) {
    print(name); // Menampilkan nama yang mengandung "ANDIL"
  });

  // Menggunakan fungsi anonim untuk mengubah nama menjadi huruf kecil
  var lowerCaseNames = names.map((name) => name.toLowerCase()).toList();

  print('Nama dalam huruf kecil:');
  lowerCaseNames.forEach((name) {
    print(name); // Menampilkan nama dalam huruf kecil
  });
}
