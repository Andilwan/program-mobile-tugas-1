void main() {
  // Membuat list dengan nama "Andil" untuk menyimpan daftar buku
  List<String> AndilBooks = ['Belajar Dart', 'Pemrograman Flutter', 'Pengembangan Web'];

  // Menampilkan isi list
  print('Daftar Buku Andil: ' + ferdiBooks.toString());

  // Menambahkan buku baru
  AndilBooks.add('Dasar-Dasar Algoritma');
  print('Setelah menambahkan buku baru: ' + AndilBooks.toString());

  // Menghapus buku
  AndilBooks.remove('Pemrograman Flutter');
  print('Setelah menghapus buku: ' + AndilBooks.toString());

  // Mengakses elemen list
  print('Buku pertama Andil: ' + AndilBooks[0]);

  // Menampilkan jumlah buku
  print('Jumlah buku Andil: ' + AndilBooks.length.toString());
}
